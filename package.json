  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djansyle/g-apply/issues"
  },
  "homepage": "https://github.com/djansyle/g-apply#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
